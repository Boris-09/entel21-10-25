{
	"name": "df_union_csvs",
	"properties": {
		"folder": {
			"name": "Bronze"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_llamadas_entel_csv03",
						"type": "DatasetReference"
					},
					"name": "csv03"
				},
				{
					"dataset": {
						"referenceName": "ds_llamadas_entel_csv05",
						"type": "DatasetReference"
					},
					"name": "csv05"
				},
				{
					"dataset": {
						"referenceName": "ds_llamadas_entel_csv06",
						"type": "DatasetReference"
					},
					"name": "csv06"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_llamadas_entel_csvCompletos",
						"type": "DatasetReference"
					},
					"name": "SinkCombinado"
				}
			],
			"transformations": [
				{
					"name": "Csv03ConCsv05"
				},
				{
					"name": "UnionFinal"
				}
			],
			"scriptLines": [
				"source(output(",
				"          SERVICIO as string,",
				"          REGISTRO as string,",
				"          NUMERO_A as string,",
				"          IMEI_A as string,",
				"          {RBS UTILIZADA_A} as string,",
				"          CELDA_A as string,",
				"          LATITUD_LONGITUD_A as string,",
				"          NUMERO_B as string,",
				"          IMEI_B as string,",
				"          RBS_UTILIZADA_B as string,",
				"          CELDA_B as string,",
				"          LATITUD_LONGITUD_B as string,",
				"          FECHA as string,",
				"          DURACION as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> csv03",
				"source(output(",
				"          {\"FECHA_INICIO\";\"FECHA_FIN\";\"NUMERO\";\"IMEI\";\"RBS_UTILIZADA\";\"CELDA\";\"LATITUD LONGITUD\";\"SUBIDOS\";\"DESCARGADOS\";\"TOTAL\"} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> csv05",
				"source(output(",
				"          {\"FECHA_INICIO\";\"FECHA_FIN\";\"NUMERO\";\"IMEI\";\"RBS_UTILIZADA\";\"CELDA\";\"LATITUD LONGITUD\";\"SUBIDOS\";\"DESCARGADOS\";\"TOTAL\"} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> csv06",
				"csv03, csv05 union(byName: true)~> Csv03ConCsv05",
				"Csv03ConCsv05, csv06 union(byName: true)~> UnionFinal",
				"UnionFinal sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          SERVICIO as string,",
				"          REGISTRO as string,",
				"          NUMERO_A as string,",
				"          IMEI_A as string,",
				"          {RBS UTILIZADA_A} as string,",
				"          CELDA_A as string,",
				"          LATITUD_LONGITUD_A as string,",
				"          NUMERO_B as string,",
				"          IMEI_B as string,",
				"          RBS_UTILIZADA_B as string,",
				"          CELDA_B as string,",
				"          LATITUD_LONGITUD_B as string,",
				"          FECHA as string,",
				"          DURACION as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SinkCombinado"
			]
		}
	}
}